{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/*", "src/preload/*", "src/**/*.d.ts", "src/constant/*"],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "paths": {
      "@constant/*": [
        "src/constant/*"
      ]
    },
  }
}
